    <!-- http://purl.bdrc.io/ontology/WorkHolding -->

    <owl:Class rdf:about="http://purl.bdrc.io/ontology/WorkHolding">
        <rdfs:subClassOf rdf:resource="http://purl.bdrc.io/ontology/Facet"/>
        <rdfs:comment xml:lang="en">A holding refers to the library Place which has one or more physical copies of the  catalogued Work and shows the number of copies that are held at the library. The CData for the Holding is the Library&apos;s shelving information concerning the location in the library of the physical copies. The exception is an optional note regarding missing volumes in a copy or other information such as may  be useful to qualify the status of one or more copies.

Example:

bdr:W1TLMTGA000883 a :Work ;
      :workHasHolding [ a :WorkHolding ;
            :workHoldingLibrary bdr:G1TLMTGA000001 ;
            :workHoldingCopies 1 ;
            :workHoldingShelf &quot;||||001278&quot; ;
            :workHoldingCode &quot;TGA&quot; ] ;
      :workHasHolding [ a :WorkHolding ;
            :workHoldingLibrary bdr:G1TLMTGB000001 ;
            :workHoldingCopies 1 ;
            :workHoldingShelf &quot;||||000026&quot; ;
            :workHoldingCode &quot;TGB&quot; ] .

bdr:W20907 a :Work ;
      :workHasHolding [ a :WorkHolding ;
            :workHoldingLibrary bdr:G1TLMTGA000001 ;
            :workHoldingCopies 1 ;
            :workHoldingShelf &quot;&gt;||||000567&quot; ;
            :workHoldingCode &quot;TGA&quot; ] .

bdr:W1KG20928 a :Work ;
      :workHasHolding [ a :WorkHolding ;
            :workHoldingLibrary bdr:G1TLMTGA000001 ;
            :workHoldingCopies 1 ;
            :workHoldingShelf &quot;&gt;||||001811&quot; ;
            :workHoldingCode &quot;TGA&quot; ] ;
            :workHoldingException &quot;&quot;@bo .</rdfs:comment>
        <rdfs:label xml:lang="en">Holding</rdfs:label>
    </owl:Class>
    


