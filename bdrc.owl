     xml:base="http://purl.bdrc.io/ontology/"
     xmlns:role="http://purl.bdrc.io/ontology/role#"
     xmlns:bf="http://id.loc.gov/ontologies/bibframe/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:out="http://purl.bdrc.io/ontology/outline#"
     xmlns:lin="http://purl.bdrc.io/ontology/lineage#"
     xmlns:vol="http://purl.bdrc.io/ontology/volumes#"
     xmlns:top="http://purl.bdrc.io/ontology/topic#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:root="http://purl.bdrc.io/ontology/root#"
     xmlns:dcterms="http://purl.org/dc/terms/"
     xmlns:per="http://purl.bdrc.io/ontology/person#"
     xmlns:crp="http://purl.bdrc.io/ontology/corporation#"
     xmlns:prd="http://purl.bdrc.io/ontology/product#"
     xmlns:bdd="http://purl.bdrc.io/data/"
     xmlns:plc="http://purl.bdrc.io/ontology/place#"
     xmlns:adm="http://purl.bdrc.io/ontology/admin/"
     xmlns:bdo="http://purl.bdrc.io/ontology/"
     xmlns:ofc="http://purl.bdrc.io/ontology/office#"
     xmlns:bdr="http://purl.bdrc.io/resource/"
