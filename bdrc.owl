        <rdfs:seeAlso rdf:resource="http://purl.bdrc.io/ontology/Lineage"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.bdrc.io/ontology/lineageSite -->

    <owl:ObjectProperty rdf:about="http://purl.bdrc.io/ontology/lineageSite">
