        <rdfs:range rdf:resource="http://purl.bdrc.io/ontology/PaginationType"/>
        <rdfs:label xml:lang="en">pagination</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://purl.bdrc.io/ontology/PaginationType"/>
