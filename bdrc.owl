    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.bdrc.io/ontology/dataTypeExpression -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/dataTypeExpression">
        <skos:definition xml:lang="en">Defines a regex that validates a subclass of xsd:string.</skos:definition>
        <skos:prefLabel xml:lang="en">DataType Expression</skos:prefLabel>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/external -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/external">
        <rdfs:range rdf:resource="http://purl.bdrc.io/ontology/common#External"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/inferSubTree -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/inferSubTree">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/log_entry -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/log_entry">
        <rdfs:range rdf:resource="http://purl.bdrc.io/ontology/common#LogEntry"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/root#Resource"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/migration_exception -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/migration_exception">
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/note -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/note">
        <rdfs:comment>the :note annotation adds langauge tagged string and optionally a reference to a Work and a simpe text description of a location in a Work, e.g., &quot;pp. 231-3&quot;.

In the XML db the notes are restricted to a limited set of items such as top-level resources and Outline Nodes, Place Events and so on. In this model there are no limitations on where notes can be attached. They can be attached directly to any owl:NamedIndividual or property. In particular one could add a note to the per:name property on an individual that provides evidence for the particular per:name assertion.</rdfs:comment>
        <rdfs:range rdf:resource="http://purl.bdrc.io/ontology/Note"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/seeAlso -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/seeAlso">
        <rdfs:range rdf:resource="http://purl.bdrc.io/ontology/topic#Topic"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/topic#Topic"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/status -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/status">
        <rdfs:range rdf:resource="http://purl.bdrc.io/ontology/StatusType"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/Entity"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/taxElementOfRelation -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/taxElementOfRelation">
        <rdfs:comment xml:lang="en">Identifies a relation to be used as the element of relation for a custom taxonomy</rdfs:comment>
        <rdfs:label xml:lang="en">taxonomy element of relation</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/Taxonomy"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/taxSubclassRelation -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/taxSubclassRelation">
        <rdfs:comment xml:lang="en">This identifies a property that is used in a custom taxonomy to represent the subclass relation for the taxonomy</rdfs:comment>
        <rdfs:label xml:lang="en">taxonomy subclass relation</rdfs:label>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
        <rdfs:domain rdf:resource="http://purl.bdrc.io/ontology/Taxonomy"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/conversionAnnotations -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/conversionAnnotations"/>
    


    <!-- http://purl.bdrc.io/ontology/admin/convertProperty -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/convertProperty">
        <rdfs:comment xml:lang="en">this annotation indicates that the annotated property is to be converted to some other expression, typically to a form that is expressed in terms of a common 3rd-party ontology such as bibframe or foaf.

The value of this property should be an explanation of the needed conversion as an rdf:langString</rdfs:comment>
        <skos:prefLabel xml:lang="en">Admin convertProperty</skos:prefLabel>
        <rdfs:subPropertyOf rdf:resource="http://purl.bdrc.io/ontology/admin/conversionAnnotations"/>
        <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/convertToClass -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/convertToClass">
        <rdfs:comment xml:lang="en">this annotation is used to indicate that uses of the annotated class are to be replaced by the inidcated class</rdfs:comment>
        <rdfs:label xml:lang="en">convert to Class</rdfs:label>
        <rdfs:subPropertyOf rdf:resource="http://purl.bdrc.io/ontology/admin/conversionAnnotations"/>
        <rdfs:range rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
        <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/convertToProperty -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/convertToProperty">
        <rdfs:comment xml:lang="en">Indicates that the annotated property is to be converted directly to a statement in terms of another property. The value of this annotation is the target property to convert to.</rdfs:comment>
        <skos:prefLabel xml:lang="en">Admin convertToProperty</skos:prefLabel>
        <rdfs:subPropertyOf rdf:resource="http://purl.bdrc.io/ontology/admin/conversionAnnotations"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.bdrc.io/ontology/admin/convertValueToClass -->

    <owl:AnnotationProperty rdf:about="http://purl.bdrc.io/ontology/admin/convertValueToClass">
        <rdfs:comment xml:lang="en">Indicates that the value of the property being converted is to be an instance of the designated class</rdfs:comment>
        <skos:prefLabel xml:lang="en">Admin convertValueToClass</skos:prefLabel>
        <rdfs:subPropertyOf rdf:resource="http://purl.bdrc.io/ontology/admin/conversionAnnotations"/>
    </owl:AnnotationProperty>
    


    <!-- http://purl.org/dc/terms/modified -->

