    <!-- http://purl.bdrc.io/ontology/ISBN -->

    <rdfs:Datatype rdf:about="http://purl.bdrc.io/ontology/ISBN">
        <dataTypeExpression>^(97(8|9))?\d{9}(\d|X)$</dataTypeExpression>
        <rdfs:comment xml:lang="en">The regex is does not take into account opitional blank, &apos; &apos;, or dash, &apos;-&apos;, chars separating the various groups. Also there is no spec for calculating the check digit</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="https://www.isbn-international.org/sites/default/files/ISBN%20Manual%202012%20-corr.pdf"/>
        <rdfs:label xml:lang="en">International Standard Book Number</rdfs:label>
    </rdfs:Datatype>
    


    <!-- http://purl.bdrc.io/ontology/LCCN -->

    <rdfs:Datatype rdf:about="http://purl.bdrc.io/ontology/LCCN">
        <dataTypeExpression>[[a-z]|[[a-z][a-z]|dd]|[a-z][[a-z][a-z]|dd]|[a-z][a-z]dd]dddddddd</dataTypeExpression>
        <rdfs:isDefinedBy rdf:resource="http://www.loc.gov/marc/lccn-namespace.html"/>
        <rdfs:label xml:lang="en">Library of Congress Control Number</rdfs:label>
        <owl:equivalentClass rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    </rdfs:Datatype>
    


    <!-- http://purl.bdrc.io/ontology/LCCallNumber -->

    <rdfs:Datatype rdf:about="http://purl.bdrc.io/ontology/LCCallNumber">
        <dataTypeExpression rdf:resource="https://gist.github.com/kurtraschke/560162"/>
        <rdfs:isDefinedBy rdf:resource="https://www.loc.gov/aba/publications/FreeCSM/freecsm.html"/>
        <rdfs:label xml:lang="en">Library of Congress Call Number</rdfs:label>
        <rdfs:seeAlso rdf:resource="https://www.loc.gov/catdir/cpso/lcco/"/>
    </rdfs:Datatype>
    


