        <rdfs:label xml:lang="en">lineage type</rdfs:label>
        <rdfs:seeAlso rdf:resource="http://purl.bdrc.io/ontology/Lineage"/>
    </owl:ObjectProperty>
    


    <!-- http://purl.bdrc.io/ontology/lineageWho -->

