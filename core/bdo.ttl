bdo:workIsAbout
	a owl:ObjectProperty ;
	rdfs:label "is about"@en ;
	adm:userTooltip "The referenced Entity (topic, person, work, etc.) is a subject of the Work - something the Work is about."@en ;
	rdfs:domain bdo:Work ;
	rdfs:range bdo:Entity ;
	.

bdo:workSeriesNumber
	a owl:DatatypeProperty ;
	rdfs:label "issue"@en ;
	adm:userTooltip "This is the number or other id of this SerialInstance in a SerialWork."@en ;
	rdfs:domain bdo:SerialInstance ;
	rdfs:range xsd:string ;
	owl:deprecated "true"^^xsd:boolean ;
	.

bdo:workSideBoxSequenceId
	a owl:DatatypeProperty ;
	rdfs:label "side box sequence id"@en ;
	adm:catalogingConvention "An identifer, typically a Tibetan letter, indicating the position of the text in a sequence of texts. Conventionally appearing in a side box of the textbox. See bdr:W22080 for an example."@en ;
	adm:userTooltip "An identifer, typically a Tibetan letter, indicating the position of the text in a sequence of texts."@en ;
	rdfs:domain bdo:Instance ;
	rdfs:range rdf:langString ;
	.

bdo:workTranslationOf
	a owl:ObjectProperty ;
	rdfs:subPropertyOf bdo:workDerivativeOf ;
	rdfs:label "translation of"@en ;
	adm:catalogingConvention "The subject work is identified as a translation of the referenced work. This will be used, for example, when the subject work is a Tibetan translation of a referenced Sanskrit work."@en ;
	adm:userTooltip "This work is a translation of the reference work."@en ;
	rdfs:domain bdo:Work ;
	rdfs:range bdo:Work ;
	owl:inverseOf bdo:workHasTranslation ;
	.

bdo:yearOffset
	a owl:DatatypeProperty ;
	rdfs:label "year offset"@en ;
	adm:catalogingConvention "This is the year offset for an Era in terms of the Common Era and is established based on the definition of the Era for which this is the offset"@en ;
	adm:userTooltip "This is the year offset for an Era in terms of the Common Era."@en ;
	rdfs:domain bdo:Era ;
	rdfs:range xsd:integer ;
	.

bdr:ChineseDateNotSpecified
	a bdo:ChineseDate ;
	rdfs:label "Unspecified Chinese date"@en ;
	adm:translationPriority "2"^^xsd:integer ;
	adm:userTooltip "An unspecified Chinese date."@en ;
	.

bdr:LanguageTaxonomy
	a bdo:Type ;
	.

